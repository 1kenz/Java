package variables;

public class Main {

	public static void main(String[] args) {
		// case sensitive
		// camel casing
		// reusability
		// cleancode
		// type convertion
		String message = "Number of Students :";
		int number = 10;
		System.out.println("Number of Students : " + number);
		System.out.println(message + number);
		

	}

}
